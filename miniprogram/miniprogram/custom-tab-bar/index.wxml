<!--custom-tab-bar/index.wxml-->
<van-tabbar active="{{ active }}" active-color="#335274"
  inactive-color="#999" safe-area-inset-bottom="{{ true }}" bind:change="onChange">
  <van-tabbar-item wx:for="{{ list }}" wx:key="index" icon="{{ item.icon }}" info="{{ item.info }}" >
    {{item.text}}
  </van-tabbar-item>
</van-tabbar>